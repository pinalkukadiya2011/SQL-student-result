<form method="post">
    <table>
        <tr>
            <th>
                STUDENT RESULT
            </th>
        </tr>
        <tr>
            <td>Name</td>
            <td><input type="text" name="name" placeholder="ENTER THE NAME"/> </td>
        </tr>
        <tr>
            <td>sub 1</td>
            <td><input type="int" name="sub1" placeholder="ENTER THE SUB 1"/> </td>
        </tr> 
        <tr>
            <td>sub 2</td>
            <td><input type="int" name="sub2" placeholder="ENTER THE SUB 2"/> </td>
        </tr>
        <tr>
            <td>sub 3</td>
            <td><input type="int" name="sub3" placeholder="ENTER THE SUB 3"/> </td>
        </tr>
        <tr>
            <td>sub 4</td>
            <td><input type="int" name="sub4" placeholder="ENTER THE SUB 4"/> </td>
        </tr>
        <tr>
            <td>sub 5</td>
            <td><input type="int" name="sub5" placeholder="ENTER THE SUB 5"/> </td>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" name="submit"/> </td>
        </tr>
    </table>
</form>

<table>
    <tr>
        <th>
            INFORMATION
        </th>
    </tr>
    <tr>
        <td>ROLL NO</td>
        <td>NAME</td>
        <td>SUB 1</td>
        <td>SUB 2</td>
        <td>SUB 3</td>
        <td>SUB 4</td>
        <td>SUB 5</td>
        <td>TOTAL</td>
        <td>AVG</td>
        <td>MAX</td>
        <td>MIN</td>
        <td>STATUS</td>
        <td>ACTION</td>
        <td>ACTION</td>
       
    </tr>
<% results.forEach(function(stu1){%>

    <tr>
      <td><%= stu1.r_no%></td>
      <td><%= stu1.name%></td>
      <td><%= stu1.sub1%></td>
      <td><%= stu1.sub2%></td>
      <td><%= stu1.sub3%></td>
      <td><%= stu1.sub4%></td>
      <td><%= stu1.sub5%></td>
      <td><%= stu1.total%></td>
      <td><%= stu1.avg%></td>
      <td><%= stu1.max%></td>
      <td><%= stu1.min%></td>
      <td><%= stu1.status%></td>
      <td><a href="/delete/<%= stu1.r_no%>">DELETE</a></td>
      <td><a href="/update/<%= stu1.r_no%>">UPDATE</a></td>
    </tr>



<% }) %>


<form method="get" action="/top">
    <button type="submit">TOP STUDENTS</button>
   
</form>
<form method="get" action="/fail">
    <button type="submit">FAIL</button>
</form>
<form method="get" action="/pass">
    <button type="submit">PASS</button>
</form>
<form method="get" action="/atkt">
    <button type="submit">ATKT</button>
</form>


</table>